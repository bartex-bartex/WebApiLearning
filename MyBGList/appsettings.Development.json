{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "MyBGList": "Debug"
      }
    }
  },
  "AllowedHosts": "*",
  "UseDeveloperExceptionPage": false,
  "UseSwagger": true,
  "AllowedOrigins": "localhost"
}
